<tr>
    <td colspan="{{=it.post.fileInfos.length + 2}}" class="vote">
        <div name="voteText" class="voteText">
            {{? it.disabled}}
                <img name="voteClosedImg" src="/{{=it.site.pathPrefix}}img/closed.png" title="{{=it.tr.voteClosedText}}">
            {{?}}
            {{=it.text}}</div>
        {{? !it.disabled && !it.post.ownIp}}
            <form method="post" accept-charset="utf-8" enctype="multipart/form-data" class="inlineForm"
                  action="/{{=it.site.pathPrefix}}action/{{? it.voted}}unvote{{??}}vote{{?}}"
                  {{? it.settings.mode.name != "ascetic"}}onsubmit="lord.vote(event, this); return false;"{{?}}>
        {{?}}
        <div name="voteVariants">
            {{ for (var i = 0; i < it.variants.length; ++i) { }}
                {{ var variant = it.variants[i]; }}
                {{? it.multiple}}
                    <input type="checkbox" name="voteVariant_{{=variant.id}}" value="true"
                {{??}}
                    <input type="radio" name="voteGroup" value="{{=variant.id}}"
                {{?}}
                {{? it.post.ownIp || it.disabled || it.voted}}
                    disabled="true"
                {{?}}
                {{? variant.ownIp}}
                    checked="true"
                {{?}}
                />
                {{=variant.text}}
                <span class="voteCount">({{=it.tr.voteCountText}} {{ out += (variant.voteCount || 0); }})</span></br />
            {{ } }}
        </div>
        {{? !it.post.ownIp}}
            <input type="hidden" name="postNumber" value="{{=it.post.number}}" />
            <input type="image" src="/{{=it.site.pathPrefix}}img/{{? !it.voted}}vote{{??}}unvote{{?}}.png"
                   name="button{{? !it.voted}}Vote{{??}}Unvote{{?}}" {{? it.disabled}}disabled="true"{{?}}
                   title="{{? !it.voted}}{{=it.tr.voteActionText}}{{??}}{{=it.tr.unvoteActionText}}{{?}}"
                   width="24" height="24" class="buttonImage" />
            </form>
        {{?}}
        <a href="/{{=it.site.pathPrefix}}{{? !it.disabled}}close{{??}}open{{?}}Voting.html?postNumber={{=it.post.number}}"
           title="{{? !it.disabled}}{{=it.tr.closeVoteActionText}}{{??}}{{=it.tr.openVoteActionText}}{{?}}"
           {{? it.settings.mode.name != "ascetic"}}onclick="lord.setVotingOpened(this, {{? !it.disabled}}false{{??}}true{{?}}); return false;"{{?}}
           target="_blank"><img src="/{{=it.site.pathPrefix}}img/{{? !it.disabled}}close{{??}}open{{?}}_voting.png"
                                width="24" height="24" class="buttonImage"></a>
    </td>
</tr>
