<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
{{#def.head}}
<body {{#def.baseData}}>
{{#def.toolbar}}
{{#def.customHeader}}
<br />
{{#def.searchAction}}
<div class="theTitle">
    <h1>
        {{=it.title}}
    </h1>
</div>
<div id="bannedUsers">
    {{ for (var ip in it.bannedUsers) { }}
        {{ if (!it.bannedUsers.hasOwnProperty(ip)) continue; }}
        {{ var user = { ip: ip, bans: it.bannedUsers[ip] }; }}
        <div id="user{{=user.ip}}">
            {{#def.userBan}}
            <br />
        </div>
    {{ } }}
</div>    
<hr />
<div>
    {{? typeof user != "undefined"}}
        {{ user = undefined; }}
    {{?}}
    {{#def.userBan}}
</div>
{{#def.customFooter}}
</body>
</html>
