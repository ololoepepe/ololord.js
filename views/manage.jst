<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
{{#def.head}}
<body class="{{=it.deviceType}}" {{#def.baseData}}>
{{? it.customHeader}}
    {{#def.customHeader}}
{{?}}
<div id="toolbarPlaceholder" style="display: none;"></div>
<br />
<div id="searchPlaceholder" style="display: none;"></div>
<div class="theTitle">
    <h1>
        {{=it.title}}
    </h1>
</div>
<div id="bannedUsers">
    {{ for (var ip in it.bannedUsers) { }}
        {{ if (!it.bannedUsers.hasOwnProperty(ip)) continue; }}
        {{ var user = { ip: ip, bans: it.bannedUsers[ip] }; }}
        <div id="user{{=user.ip}}">
            {{#def.userBan}}
            <br />
        </div>
    {{ } }}
</div>    
<hr />
<div>
    {{? typeof user != "undefined"}}
        {{ user = undefined; }}
    {{?}}
    {{#def.userBan}}
</div>
{{? it.customFooter}}
    {{#def.customFooter}}
{{?}}
</body>
</html>
