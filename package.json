{
  "name": "ololord.js",
  "version": "2.0.0-alpha3",
  "description": "Imageboard engine in JavaScript to rule them all",
  "main": "server/server.js",
  "scripts": {
    "build-all": "gulp build-all",
    "build": "gulp build",
    "build-server": "gulp build-server",
    "build-css": "gulp build-css",
    "build-js": "gulp build-js",
    "build-custom": "gulp build-custom",
    "build-custom-server": "gulp build-custom-server",
    "build-custom-css": "gulp build-custom-css",
    "build-custom-js": "gulp build-custom-js",
    "build-all-debug": "gulp build-all-debug",
    "build-debug": "gulp build-debug",
    "build-server-debug": "gulp build-server-debug",
    "build-css-debug": "gulp build-css-debug",
    "build-js-debug": "gulp build-js-debug",
    "build-custom-debug": "gulp build-custom-debug",
    "build-custom-server-debug": "gulp build-custom-server-debug",
    "build-custom-css-debug": "gulp build-custom-css-debug",
    "build-custom-js-debug": "gulp build-custom-js-debug",
    "build-docs": "npm run build-docs-server && npm run build-docs-js",
    "build-docs-server": "node_modules/.bin/jsdoc --configure docs/config/server.json --verbose",
    "build-docs-js": "node_modules/.bin/jsdoc --configure docs/config/js.json --verbose",
    "build-custom-docs": "npm run build-custom-docs-server && npm run build-custom-docs-js",
    "build-custom-docs-server": "node_modules/.bin/jsdoc --configure docs/config/custom-server.json --verbose",
    "build-custom-docs-js": "node_modules/.bin/jsdoc --configure docs/config/custom-js.json --verbose",
    "translate": "./node_modules/cute-localize/bin/clupdate.js -e js,jst,def -l ru -d \"TRANSLATE_ME\" -X \"$(pwd)/public\",\"$(pwd)/server\",\"$(pwd)/views\",\"$(pwd)/node_modules\"",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "/usr/bin/env node server/server.js",
    "start-renderer": "/usr/bin/env node server/server-renderer.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ololoepepe/ololord.js.git"
  },
  "keywords": [
    "imageboard"
  ],
  "author": "Andrey Bogdanov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ololoepepe/ololord.js/issues"
  },
  "homepage": "https://github.com/ololoepepe/ololord.js",
